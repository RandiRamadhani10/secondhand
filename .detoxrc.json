{
    "testRunner": "jest",
    "runnerConfig": "e2e/config.json",
    "skipLegacyWorkersInjection": true,
    "apps": {
      "ios": {
        "type": "ios.app",
        "binaryPath": "ios/build/Build/Products/Release-iphonesimulator/secondhand.app",
        "build": "xcodebuild -project ios/secondhand.xcodeproj -scheme secondhand -sdk iphonesimulator -derivedDataPath ios/build"
      },
      "android.debug": {
        "type": "android.apk",
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "cd android && gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && cd .."
      },
      "android.release": {
        "type": "android.apk",
        "binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
        "build": "cd android && gradlew assembleRelease assembleAndroidTest -DtestBuildType=release && cd .."
      }
    },
    "devices": {
      "simulator": {
        "type": "ios.simulator",
        "device": {
          "type": "iPhone 11"
        }
      },
      "emulator": {
        "type": "android.emulator",
        "device": {
          "avdName": "ANDROID Q"
        }
      },
      "android.attached": {
        "type": "android.attached",
        "device": {
          "adbName": "ANDROID Q"
        }
      }
    },
    "configurations": {
      "ios": {
        "device": "simulator",
        "app": "ios"
      },
      "android.emu.debug": {
        "device": "android.attached",
        "app": "android.debug"
      },
      "android.emu.release": {
        "device": "android.attached",
        "app": "android.release"
      },
      "android.attached.release": {
        "device": "android.attached",
        "app": "android.release"
      }
    }
  }
  